<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>fb-monopoly</application>
	<version>1</version>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	
	<!-- Register application for notifications when a client connects or disconnected from channel  -->
	<inbound-services>
    	<service>channel_presence</service>
	</inbound-services>
	
</appengine-web-app>